<% if (typeof(isHead) !== 'undefined' && isHead) { %>
    <% if (theme.plugins.lightgallery) { %>
        <%- css('//cdn.staticfile.org/lightgallery/1.6.11/css/lightgallery.min') %>
    <% } %>
    <% if (theme.plugins.justifiedgallery) { %>
        <%- css('//cdn.staticfile.org/justifiedGallery/3.7.0/css/justifiedGallery.min') %>
    <% } %>
    <% if (theme.plugins.google_analytics) { %>
        <%- partial('plugin/google-analytics') %>
    <% } %>
    <% if (theme.plugins.google_site_verification) { %>
        <meta name="google-site-verification" content="<%= theme.plugins.google_site_verification %>" />
    <% } %>
    <% if (theme.plugins.baidu_analytics) { %>
        <%- partial('plugin/baidu-analytics') %>
    <% } %>
<% } else { %>
    <% if (theme.plugins.lightgallery) { %>
        <%- js('//cdn.staticfile.org/lightgallery/1.6.11/js/lightgallery.min') %>
        <%- js('//cdn.staticfile.org/lg-thumbnail/1.1.0/lg-thumbnail.min')%>
        <%- js('//cdn.staticfile.org/lg-pager/1.0.2/lg-pager.min')%>
        <%- js('//cdn.staticfile.org/lg-autoplay/1.0.4/lg-autoplay.min')%>
        <%- js('//cdn.staticfile.org/lg-fullscreen/1.0.1/lg-fullscreen.min')%>
        <%- js('//cdn.staticfile.org/lg-zoom/1.1.0/lg-zoom.min')%>
        <%- js('//cdn.staticfile.org/lg-hash/1.0.4/lg-hash.min')%>
        <%- js('//cdn.staticfile.org/lg-share/1.1.0/lg-share.min')%>
        <%- js('//cdn.staticfile.org/lg-video/1.2.2/lg-video.min')%>
    <% } %>
    <% if (theme.plugins.justifiedgallery) { %>
        <%- js('//cdn.staticfile.org/justifiedGallery/3.7.0/js/jquery.justifiedGallery.min') %>
    <% } %>
    <% if (theme.plugins.mathjax) { %>
        <script type="text/x-mathjax-config">
            MathJax.Hub.Config({ tex2jax: { inlineMath: [['$','$'], ['\\(','\\)']] } });
        </script>
        <%- js('https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.1/MathJax.js?config=TeX-MML-AM_CHTML') %>
    <% } %>
<% } %>
